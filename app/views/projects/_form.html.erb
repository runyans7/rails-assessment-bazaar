<%= form_for @project do |f| %>
  <div class"form-group">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div class"form-group">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div class"form-group">
    <%= f.label :is_collaborator %>
    <%= f.text_area :is_collaborator %>
  </div>
  <%= f.submit 'Create Project', class: 'btn btn-primary' %>
<% end %>
